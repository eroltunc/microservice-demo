{
  "Routes": [
    {
      "UseServiceDiscovery": true,
      "ServiceName": "ErpBoard.Services.IdentityService",
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      //"DownstreamHostAndPorts": [
      //  {
      //    "Host": "localhost",
      //    "Port": 5051
      //  }
      //],
      "UpstreamPathTemplate": "/identity/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Delete", "Put" ],
      "UpstreamScheme": "http"
    },
    {

      "UseServiceDiscovery": true,
      "ServiceName": "ErpBoard.Services.NetsisService",
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      //"DownstreamHostAndPorts": [
      //  {
      //    "Host": "localhost",
      //    "Port": 5052
      //  }
      //],
      "UpstreamPathTemplate": "/netsis/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Delete", "Put" ],
      "UpstreamScheme": "http"
    }
   
  ],



  "GlobalConfiguration": {
    //"BaseUrl": "http://webapigateway.api:5000",
    "BaseUrl": "http://localhost:5050",
    "ServiceDiscoveryProvider": {
      //"Host": "c_consul",
      "Host": "localhost",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}